  object qRelationships: TUniQuery
    Connection = dmAxiom.uniInsight
    SQL.Strings = (
      'SELECT'
      
        '  r.forward, r.relationship, r.backward, r.rowid, r.backwardCate' +
        'gory, r.backwardSeq,'
      
        '  r.forwardCategory, r.forwardSeq, r.linkTable, r.nunique, r.bac' +
        'kward as search, r.nchild FROM'
      '  relationship r'
      'WHERE'
      '  r.nparent = :search'
      'AND'
      '  r.nchild = :nchild')
    Left = 101
