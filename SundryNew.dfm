  object qrySundry: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO SUNDRY'
      
        '  (NSUNDRY, ACCT, CREATED, BILLED, DESCR, NMATTER, NCLIENT, NTRA' +
        'NS, UNITS, NMEMO, TYPE, PRIVATE, FILEID, NCHEQUE, TAXCODE, REFNO' +
        ', AMOUNT, PERUNIT, TAX)'
      'VALUES'
      
        '  (:NSUNDRY, :ACCT, :CREATED, :BILLED, :DESCR, :NMATTER, :NCLIEN' +
        'T, :NTRANS, :UNITS, :NMEMO, :TYPE, :PRIVATE, :FILEID, :NCHEQUE, ' +
        ':TAXCODE, :REFNO, :AMOUNT, :PERUNIT, :TAX)')
    SQLDelete.Strings = (
      'DELETE FROM SUNDRY'
      'WHERE'
      '  NSUNDRY = :NSUNDRY')
    SQLUpdate.Strings = (
      'UPDATE SUNDRY'
      'SET'
      '  NSUNDRY = :NSUNDRY,'
      '  ACCT = :ACCT,'
      '  CREATED = :CREATED,'
      '  BILLED = :BILLED,'
      '  DESCR = :DESCR,'
      '  NMATTER = :NMATTER,'
      '  NCLIENT = :NCLIENT,'
      '  NTRANS = :NTRANS,'
      '  UNITS = :UNITS,'
      '  NMEMO = :NMEMO,'
      '  TYPE = :TYPE,'
      '  PRIVATE = :PRIVATE,'
