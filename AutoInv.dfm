    object qryMatters: TUniQuery
    SQL.Strings = (
            
              'SELECT MAX(M.TITLE) AS TITLE, MAX(M.FILEID) AS FILEID, MAX(M.SHO' +
              'RTDESCR) AS SHORTDESCR,'
            
              '  MAX(M.NMATTER) AS NMATTER, MAX(M.TYPE) AS TYPE, MAX(M.AUTHOR) ' +
              'AS AUTHOR,'
            
              '  MAX(M.PARTNER) AS PARTNER, MAX(M.BILLING_PLAN) AS BILLING_PLAN' +
              ','
            
              '  SUM(B.UNBILL_FEES) AS UNBILL_FEES, SUM(B.UNBILL_DISB) AS UNBIL' +
              'L_DISB,'
            
              '  SUM(B.UNBILL_ANTD) AS UNBILL_ANTD, SUM(B.UNBILL_SUND) AS UNBIL' +
              'L_SUND'
            'FROM MATTER M, AUTOBILL B'
            'WHERE M.NMATTER = B.NMATTER'
            'GROUP BY B.NMATTER'
            '')
    OnCalcFields = qryMattersCalcFields
    Left = 58
    Top = 46
