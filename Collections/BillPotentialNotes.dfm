  object qPeriod: TUniQuery
    Connection = dmAxiom.uniInsight
    SQL.Strings = (
      'SELECT DECODE (fin_period,'
      '               1, '#39'July'#39','
      '               2, '#39'August'#39','
      '               3, '#39'September'#39','
      '               4, '#39'October'#39','
      '               5, '#39'November'#39','
      '               6, '#39'December'#39','
      '               7, '#39'January'#39','
      '               8, '#39'February'#39','
      '               9, '#39'March'#39','
      '               10, '#39'April'#39','
      '               11, '#39'May'#39','
      '               12, '#39'June'#39
      '              ) || '#39' - '#39' ||cal_year as period, ncalendar'
      '  FROM calendar'
      '  where cal_year = :pcal_year'
      '  order by fin_year, cal_month')
    Left = 329
    Top = 21
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'pcal_year'
        Value = nil
      end>
  end
