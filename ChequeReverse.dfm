  end
  object qryCreditorCheqReversal: TUniQuery
    Connection = dmAxiom.uniInsight
    SQL.Strings = (
      'update invoice i set owing = owing + '
      '(select '
      't.amount * -1 from transitem t, chart c  where'
      't.ncheque = :ncheque '
      'and t.ninvoice = i.ninvoice '
      'and owner_code = '#39'CHEQUE'#39' '
      'and t.chart = c.code '
      'and t.acct = c.bank '
      'and c.charttype <> '#39'BANK'#39')'
      
        'where exists (select 1 from transitem tt where tt.ncheque = :nch' +
        'eque and tt.ninvoice = i.ninvoice'
      'and tt.owner_code = '#39'CHEQUE'#39')')
