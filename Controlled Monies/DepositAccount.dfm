  object qryMRUList: TUniQuery
    Connection = dmAxiom.uniInsight
    SQL.Strings = (
      
        'SELECT M.FILEID,M.NMATTER,P.SEARCH || '#39' '#39' || m.shortdescr as SEA' +
        'RCH, idx'
      'FROM MATTER M, PHONEBOOK P,OPENLIST O'
      'WHERE O.AUTHOR = :P_Author'
      'AND O.TYPE = :P_Type'
      'AND O.CODE = M.FILEID'
      'AND M.NCLIENT = P.NCLIENT'
      'union '
      
        'select M.fileid,M.NMATTER,P.SEARCH || '#39' '#39' || m.shortdescr as SEA' +
        'RCH,999'
      'from matter m,PHONEBOOK P'
      'where M.NCLIENT = P.NCLIENT'
      'and m.nmatter = :staticmatter'
      'union'
      'SELECT '#39'Search...'#39',0,'#39#39',999'
      'FROM dual'
      'ORDER BY 3')
    Left = 349
    Top = 67
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'P_Author'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'P_Type'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'staticmatter'
        Value = nil
      end>
  end
