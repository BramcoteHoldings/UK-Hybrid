  object dsSundrys: TUniDataSource
    AutoEdit = False
    DataSet = qrySundrys
    Left = 480
    Top = 20
  end
  object qrySundrys: TUniQuery
    Connection = dmAxiom.uniInsight
    SQL.Strings = (
      
        'SELECT S.CREATED, S.TYPE, S.FILEID, S.NMATTER, S.BILLED, S.DESCR' +
        ', S.NSUNDRY, S.UNITS, S.PERUNIT, S.AMOUNT, B.REFNO AS BillRefno,' +
        ' S.TAX'
      'FROM SUNDRY S, NMEMO B'
      'WHERE S.CREATED >= :P_DateFrom AND S.CREATED <= :P_DateTo'
      '  AND S.NMEMO = B.NMEMO(+)')
    Left = 447
    Top = 20
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'P_DateFrom'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'P_DateTo'
        Value = nil
      end>
  end
