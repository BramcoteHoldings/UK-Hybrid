  object qryMatterParty: TUniQuery
    DataTypeMap = <>
    SQL.Strings = (
      'SELECT MT.TITLE, MT.LONGDESCR, MP.*, PB.*'
      'FROM MATTER MT, MATTERPARTY MP, PHONEBOOK PB'
      'WHERE MP.FILEID = :FILEID'
      '  AND MT.FILEID = MP.FILEID'
      '  AND MP.NNAME = PB.NNAME'
      '  AND MP.FIELDNAME = :FIELDNAME'
      '  AND MP.CATEGORY = :CATEGORY')
    Left = 335
    Top = 5
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'FILEID'
      end
      item
        DataType = ftUnknown
        Name = 'FIELDNAME'
      end
      item
        DataType = ftUnknown
        Name = 'CATEGORY'
      end>
  end
